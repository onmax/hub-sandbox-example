{
  "name": "hub-sandbox-example",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "build:vercel": "NITRO_PRESET=vercel nuxt build",
    "build:cloudflare": "NITRO_PRESET=cloudflare_module nuxt build",
    "deploy:vercel:prod": "vercel deploy --prebuilt --prod",
