  },
  "dependencies": {
    "nuxt": "^4.3.0",
    "@nuxt/ui": "^4.4.0",
    "@nuxthub/core": "https://pkg.pr.new/@nuxthub/core@d3193ab",
    "@cloudflare/sandbox": "^0.7.0",
    "@vercel/sandbox": "^1.0.0",
    "unagent": "^0.0.5",
    "tailwindcss": "^4.1.16"
  },
  "pnpm": {
    "patchedDependencies": {
      "@nuxthub/core@0.10.5": "patches/@nuxthub__core@0.10.5.patch"
    }
  }
}
